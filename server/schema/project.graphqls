type Project {
    id: Int!
    versions: [Version]
    createdAt: String!
    updatedAt: String!
}

input createProjectInput {
    id: Int!
}

input deleteProjectInput {
    id: Int!
}

type Query {
    projects: [Project]!
    project(id: Int!): Project!
}

type Mutation {
    createProject(input: createProjectInput): Project!
    deleteProject(input: deleteProjectInput): Boolean!
}