type Version {
    id: ID!
    projectId: Int,
    informationId: ID!
    createdAt: String!
    updatedAt: String!
}

input createVersionInput {
    projectId: Int!
    informationId: ID!
}

input deleteVersionInput {
    id: ID!
}

extend type Query {
    version(id: ID!): Version!
}

extend type Mutation {
    createVersion(input: createVersionInput): Version!
    deleteVersion(input: deleteVersionInput): Boolean!
}