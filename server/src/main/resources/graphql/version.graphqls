type Version {
  id: ID!
  project: Project!
  title: String!
  description: String
  information: Information
  createdAt: String
  updatedAt: String
}

extend type Query {
  findProjectVersions(id: ID!): [Version]!
}

extend type Mutation {
  createVersion(projectId: ID!, title: String!, description: String): Version!
  deleteVersion(id: ID!): Boolean!
}
