type Version {
  id: ID!
  project: Project!
  title: String!
  description: String
  information: Information
  createdAt: String
  updatedAt: String
}

input  createVersionInput {
  projectId: ID!
  title: String!
  description: String
}

extend type Query {
  findProjectVersions(id: ID!): [Version]!
}

extend type Mutation {
  createVersion(input: createVersionInput): Version!
  deleteVersion(input: deleteInput): Boolean!
}
