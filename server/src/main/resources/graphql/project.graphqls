type Project {
  id: ID!
  versions: [Version]
  createdAt: String
  updatedAt: String
}

type Query {
  getProjects: [Project]!
  #findProjectById(id: ID!): Project!
  #countProjects: Long!
}

input createProjectInput {
  id: ID!
}

input deleteProjectInput {
  id: ID!
}

type Mutation {
  createProject(input: createProjectInput): Project!
  #updateProject(id: ID!): Project!
  deleteProject(input: deleteProjectInput!): Boolean!
}
